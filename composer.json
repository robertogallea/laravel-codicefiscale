{
    "name": "robertogallea/laravel-codicefiscale",
    "description": "Codice fiscale validation for php/laravel",
    "type": "library",
    "authors": [
        {
            "name": "Roberto Gallea",
            "email": "write@robertogallea.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.0",
        "orchestra/testbench": "^10.0.0-BETA1",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
	"autoload": {
        "psr-4": {
            "robertogallea\\LaravelCodiceFiscale\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "robertogallea\\LaravelCodiceFiscale\\CodiceFiscaleServiceProvider"
            ]
        }
    }
}
